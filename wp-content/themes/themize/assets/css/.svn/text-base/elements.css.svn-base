}
/* Paralax section */
.parallax-section {
	margin-top: 0px;
	margin-bottom: 60px !important;
	padding-top: 80px;
	padding-bottom: 80px;
	box-sizing: content-box!important;
	-moz-box-sizing: content-box!important;
	-webkit-box-sizing: content-box!important;
	margin-left: -50%;
    padding-left: 50%;
    padding-right: 50%;
    width: 100%;
    position: relative;
    z-index: 0;
    background: #fff;
    background-position: center 60%;
