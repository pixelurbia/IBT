/*-----------------------------------------------------------------------------------*/
/*	Icon Timeline
/*-----------------------------------------------------------------------------------*/

.wpb_wrapper {
	position: relative;
}
.wpb_wrapper .vc-timeline:last-child .vc-timeline-line {
	display: none;
}
.vc-timeline {
	position: relative;
}
.vc-timeline-icon {
